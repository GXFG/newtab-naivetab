{
  "name": "newtab-naivetab",
  "displayName": "NaiveTab - Newtab page",
  "type": "module",
  "version": "1.20.2",
  "private": true,
  "packageManager": "pnpm@8.0.0",
  "description": "一款简洁高效的浏览器新标签页扩展，可以高度自定义你的新标签页",
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:background": "npm run build:background -- --mode development",
    "dev:web": "vite",
    "-dev:js": "npm run build:js -- --mode development",
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:background",
    "build:prepare": "esno scripts/prepare.ts",
    "build:background": "vite build --config vite.config.background.ts",
    "build:web": "vite build",
    "-build:js": "vite build --config vite.config.content.ts",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "-pack:zip": "rimraf extension.zip && jszip-cli add extension/* -o ./extension.zip",
    "pack:zip": "rm -f extension.zip && zip -r extension.zip extension",
    "pack:crx": "crx pack extension -o ./extension.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ web-ext build --source-dir ./extension --filename extension.xpi --overwrite-dest",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension --target=firefox-desktop",
    "clear": "rimraf --glob extension/dist extension/manifest.json extension.*",
    "lint": "eslint --cache .",
    "test": "vitest test",
    "test:e2e": "playwright test",
    "postinstall": "simple-git-hooks",
    "typecheck": "tsc --noEmit",
    "buildall": "pnpm run build && pnpm run pack:zip"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^0.12.3",
    "axios": "^1.6.5",
    "cheerio": "1.0.0-rc.12",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "driver.js": "^1.3.1",
    "lunar-typescript": "^1.7.0",
    "naive-ui": "^2.37.3",
    "vue": "^3.4.10"
  },
  "devDependencies": {
    "@ffflorian/jszip-cli": "^3.6.2",
    "@iconify/json": "^2.2.167",
    "@iconify/vue": "^3.2.1",
    "@playwright/test": "^1.40.1",
    "@types/chrome": "^0.0.235",
    "@types/crypto-js": "^4.2.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18.19.6",
    "@types/webextension-polyfill": "^0.10.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@unocss/reset": "^0.51.13",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/compiler-sfc": "^3.4.10",
    "@vue/test-utils": "^2.4.3",
    "@vueuse/core": "^10.7.1",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.20.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.2.0",
    "jsdom": "^21.1.2",
    "kolorist": "^1.8.0",
    "lint-staged": "^13.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-nesting": "^11.3.0",
    "prettier": "^3.1.1",
    "rimraf": "^4.4.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unocss": "^0.51.13",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-icons": "^0.16.6",
    "unplugin-vue-components": "^0.24.1",
    "user-agent-data-types": "^0.3.1",
    "vite": "^4.5.1",
    "vite-plugin-md": "^0.21.5",
    "vitest": "^0.31.4",
    "vue-demi": "^0.14.6",
    "vue-i18n": "^9.9.0",
    "web-ext": "^7.10.0",
    "webext-bridge": "^6.0.1",
    "webextension-polyfill": "^0.10.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix",
      "git add"
    ]
  }
}

{
  "name": "newtab-naivetab",
  "displayName": "NaiveTab - Newtab page",
  "type": "module",
  "version": "1.23.4",
  "private": true,
  "packageManager": "pnpm@9.14.2",
  "description": "A simple and efficient browser new tab extension that allows you to highly customize your new tab page.",
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:background": "npm run build:background -- --mode development",
    "dev:web": "vite",
    "-dev:js": "npm run build:js -- --mode development",
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:background",
    "build:prepare": "esno scripts/prepare.ts",
    "build:background": "vite build --config vite.config.background.ts",
    "build:web": "vite build",
    "-build:js": "vite build --config vite.config.content.ts",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "-pack:zip": "rimraf extension.zip && jszip-cli add extension/* -o ./extension.zip",
    "pack:zip": "rm -f extension.zip && zip -r extension.zip extension",
    "pack:crx": "crx pack extension -o ./extension.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ web-ext build --source-dir ./extension --filename extension.xpi --overwrite-dest",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension --target=firefox-desktop",
    "clear": "rimraf --glob extension/dist extension/manifest.json extension.*",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest test",
    "test:e2e": "playwright test",
    "postinstall": "simple-git-hooks",
    "typecheck": "tsc --noEmit",
    "buildall": "pnpm run build && pnpm run pack:zip"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.0",
    "axios": "^1.7.8",
    "cheerio": "1.0.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "driver.js": "^1.3.1",
    "lunar-typescript": "^1.7.6",
    "naive-ui": "^2.40.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@ffflorian/jszip-cli": "^3.8.0",
    "@iconify/json": "^2.2.277",
    "@iconify/vue": "^4.1.2",
    "@playwright/test": "^1.49.0",
    "@stylistic/eslint-plugin": "^2.11.0",
    "@types/chrome": "^0.0.287",
    "@types/crypto-js": "^4.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.1",
    "@types/webextension-polyfill": "^0.12.1",
    "@unocss/reset": "^0.64.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^12.0.0",
    "chokidar": "^4.0.1",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "eslint": "^9.16.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.31.0",
    "esno": "^4.8.0",
    "fs-extra": "^11.2.0",
    "globals": "^15.12.0",
    "jsdom": "^25.0.1",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^10.1.1",
    "prettier": "^3.4.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "unocss": "^0.64.1",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-icons": "^0.20.2",
    "unplugin-vue-components": "^0.27.5",
    "user-agent-data-types": "^0.4.2",
    "vite": "^6.0.1",
    "vite-plugin-md": "^0.21.5",
    "vitest": "^2.1.6",
    "vue-demi": "^0.14.10",
    "vue-i18n": "^10.0.5",
    "web-ext": "^8.3.0",
    "webext-bridge": "^6.0.1",
    "webextension-polyfill": "^0.12.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix",
      "git add"
    ]
  }
}
